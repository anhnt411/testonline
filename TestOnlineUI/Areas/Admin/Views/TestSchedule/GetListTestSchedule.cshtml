@using TestOnlineModel.ViewModel.Admin
@model IEnumerable<TestScheduleViewModel>

<div class="row table">
    <!-- /.panel-heading -->
    <div class="table-responsive">
        <table class="table table-striped table-bordered table-hover">
            <thead>
                <tr>
                    <th>
                        STT
                    </th>
                    <th>
                        <a href="#" class="sortSchedule" data-sortschedule="Name"> Đợt thi</a>
                    </th>
                    <th>
                        Trạng thái 
                    </th>
                  
                    <th>
                        <a href="#" class="sortSchedule" data-sortschedule="Category">Chuyên mục</a>
                    </th>
                    <th>
                        <a href="#" class="sortSchedule" data-sortschedule="Time">Thời lượng</a>
                    </th>
                    <th>
                        <a href="#" class="sortSchedule" data-sortschedule="StartDate">Thời gian bắt đầu</a>
                    </th>
                    <th>
                        <a href="#" class="sortSchedule" data-sortschedule="EndDate">  Thời gian kết thúc</a>
                    </th>
                    <th>
                        Người tạo 
                    </th>
                    <th>
                        <a href="#">Thao tác</a>
                    </th>
                </tr>
            </thead>
            <tbody>
                @{
                    foreach (var item in Model)
                    {

                                <tr class="rows" data-totalrow="@item.TotalRecord" id="rows">
                                    <td>@item.STT </td>
                                    <td>@item.Name</td>
                                    <td>@item.Status</td>                                
                                    <td>@item.CategoryName</td>
                                    <td>@item.Time phút</td>
                                    <td>@item.StartDate</td>
                                    <td>@item.EndDate</td>
                                    <td>@item.UserCreate</td>

                                    <td width="100px"><a href="~/Admin/TestSchedule/CreateExam?scheduleId=@item.Id"><i class="fas fa-list coni"></i></a>  <a href="~/Admin/TestSchedule/AdminViewSchedule?scheduleId=@item.Id"><i class="fas fa-user-tie coni"></i></a>   <a href="#" class="deleteschedule" data-scheduleid="@item.Id" ><i class="fas fa-trash-alt coni"></i></a></td>
                                </tr>
                    }
                }


            </tbody>
        </table>



    </div>
</div>
