@using TestOnlineModel.ViewModel.Admin;
@using TestOnlineEntity.Model.Entity;
@model IEnumerable<AdminViewAccessScheduleViewModel>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
    var schedule = (TestSchedule)ViewBag.Schedule;


}

@section title{
    Danh sách thí sinh

}

<div class="row table">
    <!-- /.panel-heading -->
    <div class="table-responsive">
        <table class="table table-striped table-bordered table-hover" id="accessmember">
            <thead>
                <tr>
                    <th>
                        STT
                    </th>
                    <th>
                        Họ tên
                    </th>
                    <th>
                        Email
                    </th>
                    <th>
                        Số điện thoại
                    </th>
                    <th>
                        Địa chỉ
                    </th>
                   


                </tr>
            </thead>
            <tbody>
                @{
                    int i = 1;
                    foreach (var item in Model)
                    {
                        <tr data-examid="@item.UserExamId" data-memberid="@item.MemberId">
                            <td>@i</td>
                            <td>@item.FullName</td>
                            <td>@item.Email</td>
                            <td>@item.PhoneNumber</td>
                            <td>@item.Address</td>                             
                        </tr>
                        
                        i++;
                    }


                }


            </tbody>
        </table>

        <div class="loader" style="display:none"></div>

    </div>
</div>

<div class="row">
    <button class="btn btn-danger" id="exportaccessmember" data-filterkey="1" data-schedule="@schedule.Id">Xuất danh sách thí sinh</button>
    <button class="btn btn-danger" id="backprev1" data-filterkey="1" data-schedule="@schedule.Id">Quay lại</button>
</div>


