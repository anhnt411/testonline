
@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
    //TempData["success"] = TempData["updatesuccess"];
    var list = ViewBag.ListCategory;
    TempData["success"] = TempData["updatequestionbanksuccess"];
}

@section title{
    Danh sách ngân hàng câu hỏi


}

@section script{
    <script src="https://cdnjs.cloudflare.com/ajax/libs/twbs-pagination/1.4.2/jquery.twbsPagination.min.js"></script>
    <script src="~/js/member.js"></script>
    <script>
          $('#selectCategoryId option[value=@ViewBag.Category.Id]').attr('selected','selected');
    </script>

}

<div class="row">
    <div class="col-md-5">
        <a href="~/Admin/TestMember/Add?unitId=@ViewBag.Category.Id" class="btn btn-danger">Thêm mới ngân hàng câu hỏi</a>
      
    </div>
    <div class="col-md-3"></div>
    <div class="col-md-4">
        <select name="TestCategoryId" id="selectCategoryId">
            @{
                foreach (var item in list)
                {

                    <option value="@item.Id">@item.Name</option>
                }
            }
        </select>
        <button class="btn btn-danger" id="searchQuestionBank">Tìm kiếm</button>
    </div>

</div>



<div id="listMember">
</div>
<div id="pagination" class="pagination"></div>

